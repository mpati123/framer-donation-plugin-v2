{
  "outputDirectory": "plugins/framer/dist",
  "rewrites": [
    { "source": "/admin", "destination": "/admin/index.html" },
    { "source": "/admin/", "destination": "/admin/index.html" },
    { "source": "/api/campaigns/:path*", "destination": "/plugins/framer/api/campaigns/:path*" },
    { "source": "/api/checkout/:path*", "destination": "/plugins/framer/api/checkout/:path*" },
    { "source": "/api/donations/:path*", "destination": "/plugins/framer/api/donations/:path*" },
    { "source": "/api/settings/:path*", "destination": "/plugins/framer/api/settings/:path*" },
    { "source": "/api/upload/:path*", "destination": "/plugins/framer/api/upload/:path*" },
    { "source": "/api/webhook", "destination": "/plugins/framer/api/webhook/index.ts" }
  ],
  "functions": {
    "api/license/webhook.ts": {
      "includeFiles": "api/license/**"
    }
  },
  "crons": [
    {
      "path": "/api/cron/reminders",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/keepalive",
      "schedule": "0 12 * * *"
    }
  ]
}
